{
  "name": "data-structures",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "prebuild": "rm -rf lib",
    "dev": "npm run storybook | rollup -c --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fitfab/data-structures.git"
  },
  "keywords": [],
  "author": "Miguel <migueljulio.developer@gmail.com> (http://fitfab.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fitfab/data-structures/issues"
  },
  "homepage": "https://github.com/fitfab/data-structures#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.3",
    "@babel/preset-env": "^7.5.3",
    "@babel/preset-typescript": "^7.3.3",
    "rollup-plugin-analyzer": "^3.1.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "typescript": "^3.5.3"
  }
}
